programa {
  funcao inicio() {
    inteiro numeroUm, numeroDois 

    escreva("escreva o primeiro numero: ")
    leia(numeroUm)
 
   escreva("agora escreva o segundo: ")
   leia(numeroDois)

   se(numeroUm > numeroDois){
    escreva("o primeiro numero é o maior")
   }
   se(numeroDois > numeroUm){
    escreva("o segundo numero é o maior")
   }
  }
}
